                <Image url="@../../../../images/go_back_80px.png" />
            </image>
        </ImageView>
        <TableView fx:id="tableAffichage" layoutX="14.0" layoutY="83.0" stylesheets="@AffichageList.css" prefHeight="269.0" prefWidth="872.0">
            <columns>
                <TableColumn fx:id="colNom" prefWidth="127.0" text="Nom" />
                <TableColumn fx:id="colNote" prefWidth="127.0" text="Note" />
                <TableColumn fx:id="colDescription" prefWidth="233.0" text="Description" />
                <TableColumn fx:id="colDate" prefWidth="127.0" text="Date" />
            </columns>
        </TableView>
    </children>   
