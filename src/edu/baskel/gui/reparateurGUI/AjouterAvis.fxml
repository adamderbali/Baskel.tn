            </ImageView>
            <Rating fx:id="rate" layoutX="12.0" layoutY="54.0" />
            <Label fx:id="msg" layoutX="82.0" layoutY="119.0" />
            <TextField fx:id="descr" layoutX="10.0" layoutY="10.0" promptText="Description" />
