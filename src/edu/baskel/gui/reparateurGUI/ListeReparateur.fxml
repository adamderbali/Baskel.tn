<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>

<AnchorPane id="AnchorPane" prefHeight="885.0" prefWidth="1235.0" xmlns:fx="http://javafx.com/fxml/1" stylesheets="@AffichageList.css" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="edu.baskel.gui.reparateurGUI.ListeReparateurController">
    <children>
        <ImageView fx:id="retour" fitHeight="57.0" fitWidth="60.0" layoutX="3.0" layoutY="9.0" onMouseClicked="#Goback" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../../../../images/go_back_80px.png" />
            </image>
        </ImageView>
        <TableView fx:id="tableAffichage" layoutX="14.0" layoutY="83.0" prefHeight="468.0" prefWidth="872.0" stylesheets="@AffichageList.css">
            <columns>
                <TableColumn fx:id="colNom" prefWidth="127.0" text="Nom Locale" />
                <TableColumn fx:id="colRep" prefWidth="127.0" text="Nom Réparateur" />
                <TableColumn fx:id="colAdresse" prefWidth="118.0" text="Adresse" />
                <TableColumn fx:id="colTelephone" prefWidth="127.0" text="Téléphone" />
            </columns>
        </TableView>
    </children>
</AnchorPane>
